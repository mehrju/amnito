<span></span>

<main class="backblue" id="orderSteps">
    <input id="checkOutModelItemId" type="hidden" />
    <input id="isForginRequest" type="hidden" value="@(IsForegin.ToString().ToLower())" />

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <form action="" id="wizard">
                    <!-- SECTION 1 -->
                    <h2 class="text-hide">اطلاعات مرسوله و آدرس</h2>
                    <section class="postal-info">
                        <div class="col-md-12 col-lg-6 col-xl-6 col-sm-12">
                            <div class="form-content form-col ">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-header">
                                            <h3 style="font-size:17px">اطلاعات مرسوله و آدرس</h3>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6 col-sm-12  col-xl-6 col-lg-6">
                                        <label>محتویات @(PostItem)<span class="org-color"></span></label>
                                        <input type="text"
                                               class="form-control myTooltip"
                                               maxlength="100"
                                               placeholder="@(!IsHeavy?"مثلا 2 عدد ساعت":"مثلا محصولات کشاورزی")"
                                               id="GoodsType"
                                               name="GoodsType"
                                               data-toggle="tooltip"
                                               data-html="true"
                                               title="@("<a class=\"tooltipLink\" onClick=\"showTooltipmessage('postalBans')\" ><i class=\"fa fa-chevron-circle-left\" aria-hidden=\"true\"></i> مشاهده ممنوعات پستی</a>")">
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6">
                                        <label>وزن @(PostItem)(@(WeightType))<span class="org-color"></span> </label>
                                        <input type="text"
                                               class="form-control myTooltip"
                                               maxlength="@(IsHeavy?2:(IsForegin?6:7))"
                                               placeholder="مثلا 20"
                                               id="Weight_g"
                                               name="Weight_g"
                                               data-toggle="tooltip"
                                               data-html="true"
                                               title="@("<a class=\"tooltipLink\" onClick=\"showTooltipmessage('weightBans')\" ><i class=\"fa fa-chevron-circle-left\" aria-hidden=\"true\"></i> مشاهده محدودیت وزنی</a>")">
                                    </div>
                                    <div class="w-100"></div>
                                    <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6">
                                        <label>ارزش ریالی محتویات @(PostItem)<span class="org-color"></span></label>
                                        <input type="text"
                                               class="form-control myTooltip"
                                               maxlength="12" placeholder="ريال"
                                               required id="ApproximateValue"
                                               name="ApproximateValue"
                                               data-toggle="tooltip"
                                               data-html="true"
                                               title="ارزش مرسوله.در صورتی که ارزش دقیق مرسوله اطلاع ندارید.عدد 1 را وارد کنید" />
                                        <input type="hidden" id="CodGoodsPrice" />
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6">
                                        <label>@(!IsHeavy ? "تعداد بسته پستی" : "تعداد خودرو")<span class="org-color"></span> </label>
                                        <input type="number"
                                               max="50"
                                               min="1"
                                               class="form-control myTooltip"
                                               id="Count"
                                               name="count"
                                               data-toggle="tooltip"
                                               data-html="true"
                                               value="1"
                                               title="@(!IsHeavy?"چه تعداد بسته پستی با وزنی که اظهار کردید می خواهید ارسال کنید":"تعداد خودرو مورد نیاز")">
                                    </div>
                                </div>
                                <div class="row">
                                    @if (!IsHeavy)
                                    {

                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6">
                                            <label>حدود اندازه مرسوله(طول* عرض*ارتفاع)</label>
                                            <select class="js-example-basic-single form-control " name="KartonLafaf" id="KartonLafaf" data-toggle="tooltip" title="اگر @(PostItem) شما نیاز به کارتن دارد"></select>
                                        </div>
                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6" id="needCatonWrapper">
                                            <label>آیا به پاکت/کارتن پستی نیاز دارید؟</label>
                                            <input  type="checkbox" class="form-control" id="needCaton"/>
                                        </div>
                                        @*<dvi >*@
                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6" style="display:none">
                                            <label>نوع @(PostItem)<span class="org-color"></span> </label>
                                            <select class="js-example-basic-single form-control" name="boxType" id="boxType">
                                                <option value="-1">انتخاب کنید...</option>
                                                <option value="پاکت">پاکت(اوراق و اسناد)</option>
                                                <option value="بسته">بسته(کارتن،جعبه)</option>
                                            </select>
                                        </div>
                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6 " id="dimenationsBox" style="display:none">
                                            <label> لطفا ابعاد مرسوله را به سانتی متر وارد نمایید</label>
                                            <div class="row">
                                                <div class="col-md-4 col-sm-4 col-xl-4 col-lg-4 col-xs-4 col-4">
                                                    <input type="text" class="form-control myTooltip" maxlength="6" placeholder="طول" id="length" name="length" data-html="true" data-toggle="tooltip"
                                                           title="@("<a class=\"tooltipLink\" onClick=\"showTooltipmessage('dimensionsBans')\" ><i class=\"fa fa-chevron-circle-left\" aria-hidden=\"true\"></i>مشاهده محدودیت ابعاد</a>")">
                                                </div>
                                                <div class="col-md-4 col-sm-4 col-xl-4 col-lg-4 col-xs-4 col-4">
                                                    <input type="text" class="form-control myTooltip" maxlength="6" placeholder="عرض" id="width" name="width" data-html="true" data-toggle="tooltip"
                                                           title="@("<a class=\"tooltipLink\" onClick=\"showTooltipmessage('dimensionsBans')\" ><i class=\"fa fa-chevron-circle-left\" aria-hidden=\"true\"></i>مشاهده محدودیت ابعاد</a>")">
                                                </div>
                                                <div class="col-md-4 col-sm-4 col-xl-4 col-lg-4 col-xs-4 col-4">
                                                    <input type="text" class="form-control myTooltip" maxlength="6" placeholder="ارتفاع" id="height" name="height" data-html="true" data-toggle="tooltip"
                                                           title="@("<a class=\"tooltipLink\" onClick=\"showTooltipmessage('dimensionsBans')\" ><i class=\"fa fa-chevron-circle-left\" aria-hidden=\"true\"></i>مشاهده محدودیت ابعاد</a>")">
                                                </div>
                                            </div>
                                        </div>
                                        @*</dvi>*@
                                    }
                                    else
                                    {
                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6">
                                            <label for="UbbraTruckType">نوع خودرو<span class="org-color"></span></label>
                                            <select class="js-example-basic-single form-control" name="UbbraTruckType" id="UbbraTruckType"></select>
                                        </div>
                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6">
                                            <label for="VechileOptions">ویژگی خودرو<span class="org-color"></span></label>
                                            <select class="js-example-basic-single form-control" name="VechileOptions" id="VechileOptions"></select>
                                        </div>
                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6">
                                            <label for="UbbarPackingLoad">نوع بسته بندی بار<span class="org-color"></span></label>
                                            <select class="js-example-basic-single form-control" name="UbbarPackingLoad" id="UbbarPackingLoad">
                                                <option value="0">انتخاب کنید...</option>
                                                <option value="karton">کارتن</option>
                                                <option value="pallet">پالت</option>
                                                <option value="roll">رل</option>
                                                <option value="falleh">فله</option>
                                                <option value="bandil">بندیل</option>
                                                <option value="kiseh_gooni">کیسه-گونی</option>
                                            </select>
                                        </div>
                                        <div class="col-md-6 col-sm-12 col-xl-6 col-lg-6 field">

                                            <input type="text" id="dispatch_date" class="form-control" placeholder="تاریخ و ساعت بارگیری"
                                                   aria-label="_dispatch_date" aria-describedby="_dispatch_date">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text cursor-pointer" id="_dispatch_date" style="top:0px !important;padding: 6px;background-color: transparent;">
                                                    <img src="~/Themes/Ap/images/ApImages/calendar.png" />
                                                </span>
                                            </div>


                                        </div>
                                    }
                                </div>
                                <div class="row">
                                    <div class="col-md-12 col-xl-12 col-lg-12 col-sm-12">
                                        <label>آدرس فرستنده <span class="org-color"></span></label>
                                        <div class="map-btn-input" style="height:35px">
                                            <a class="get-btn-map_i faa-parent animated-hover myTooltip" title="انتخاب آدرس از بین آدرس هایی که قبلا با آن ثبت سفارش پستی انجام داده اید"
                                               data-toggle="tooltip" data-html="true" id="get-btn-map_i" data-val="Sender" data-type="OldAddress" aria-hidden="true">
