@model Nop.Plugin.Misc.ShippingSolutions.Domain.Tbl_Collectors

@{
    Layout = "_AdminLayout";
    //page title
    ViewBag.Title = T("Nop.Plugin.Misc.ShippingSolutions.Collector_pagetitle").Text;

    Html.AddCssFileParts(ResourceLocation.Footer, "~/Plugins/Misc.ShippingSolutions/Scripts/Select2/dist/css/select2.min.css");
    Html.AddScriptParts(ResourceLocation.Footer, "~/Plugins/Misc.ShippingSolutions/Scripts/Select2/dist/js/select2.full.min.js");


}

<form asp-controller="ManageCollector" asp-action="Create" method="post" id="category-form">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            @T("Nop.Plugin.Misc.ShippingSolutions.Collector_new")
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="List">@T("Nop.Plugin.Misc.ShippingSolutions.BackToList")</a>
            </small>
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.SaveContinue")
            </button>
        </div>
    </div>
    @await Html.PartialAsync("/Plugins/Misc.ShippingSolutions/Views/Collectors/_CreateOrUpdate.cshtml")
    
</form>