        }
        if (file == null || (ext.toLowerCase() != 'png' && ext.toLowerCase() != 'jpg' && ext.toLowerCase() != 'jpeg')) {
            alert("لطفا فایل تصویر با فرمت PNG و یا JPG انتخاب کنید");
            return false;
        }

        if (file.files && file.files[0]) {
            if ((file.files[0].size / 1024) > 150) {
                alert("حجم هر عکس می بایست کمتر از 150 کیلو بایت باشد");
                return false;
            }
            var reader = new FileReader();

            reader.onload = function (e) {
                $(`#${imageId}`).attr('src', e.target.result);
            }

            reader.readAsDataURL(file.files[0]); // convert to base64 string
        }

        $($(file).parent()).css('border', '3px solid black')
    }

