            function createCaptcha(captchaIndex) {
                debugger;
                var Id = 'captcha' + captchaIndex;
                //clear the contents of captcha3 div first
                document.getElementById(Id).innerHTML = "";
                var charsArray =
                    "0123456789";/*abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ*/
                var lengthOtp = 6;
                var captcha3 = [];
                for (var i = 0; i < lengthOtp; i++) {
                    //below code will not allow Repetition of Characters
                    var index = Math.floor(Math.random() * charsArray.length + 1); //get the next character from the array
                    if (captcha3.indexOf(charsArray[index]) == -1)
                        captcha3.push(charsArray[index]);
                    else i--;
                }
                var canv3 = document.createElement("canvas");
                canv3.id = Id;
                canv3.width = 150;
                canv3.height = 50;
                var ctx = canv3.getContext("2d");
                ctx.font = "25px Georgia";
                ctx.strokeText(captcha3.join(""), 0, 30);
                //storing captcha3 so that can validate you can save it somewhere else according to your specific requirements

                if (!captchaIndex)
                    code = captcha3.join("");
                else if (captchaIndex=='2')
                    code2 = captcha3.join("");
                else if (captchaIndex == '3')
                    code3 = captcha3.join("");

                document.getElementById(Id).appendChild(canv3); // adds the canvas to the body element
            }

